{
    "manifest_version": 3,
    "name": "LeetCode Leaderboards",
    "description": "Useful LeetCode Analytics",
    "version": "0.1.0",
    "icons": {
        "16": "assets/icon-16.png",
        "32": "assets/icon-32.png",
        "48": "assets/icon-48.png",
        "128": "assets/icon-128.png"
    },
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": ["https://leetcode.com/problems/*"],
            "js": ["contentScript.js"],
            "run_at": "document_end"
        }
    ],
    "action": {
        "default_icons": {
            "16": "assets/icon-16.png",
            "32": "assets/icon-32.png",
            "48": "assets/icon-48.png",
            "128": "assets/icon-128.png"
        },
        "default_title": "LeetCode Leaderboards",
        "default_popup": "popup.html"
    },
    "permissions": [
        "storage",
        "activeTab",
        "scripting",
        "tabs",
        "cookies"
    ],
    "host_permissions": ["https://leetcode.com/*"]
}